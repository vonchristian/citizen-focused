<%= simple_form_for @sign_up, url: :taxpayer_account_sign_ups do |f| %>
  <%= f.input :first_name, label: "First Name" %>
  <%= f.input :middle_name, label: "Middle Name" %>
  <%= f.input :last_name, label: "Last Name" %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>

  <%= f.submit "Sign Up" %>
<% end %>
